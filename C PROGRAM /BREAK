#include <stdio.h>

int main() {
    // Example of a while loop with a break condition
    int i = 1;

    while (1) {  // Infinite loop
        printf("%d\n", i);
        i++;

        if (i > 5) {
            // Exit the loop when i is greater than 5
            break;
        }
    }

    return 0;
}
